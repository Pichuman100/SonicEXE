<?xml version="1.0" encoding="UTF-8"?>

<!-- Defines the ISO project -->
<iso_project image_name="sonic.bin" cue_sheet="sonic.cue" no_xa="0">
	<!-- Data track -->
	<track type="data">
		<!-- Identifiers -->
		<identifiers
			system        = "PLAYSTATION"
			application   = "PLAYSTATION"
			volume        = "PSXFUNKIN"
			volume_set    = "PSXFUNKIN"
			publisher     = "CUCKYDEV"
			copyright     = "NINJAMUFFIN99"
