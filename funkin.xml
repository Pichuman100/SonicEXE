				<file name = "1.1.cht" type = "data" source = "iso/songs/too-slow/too-slow-hard.json.cht"/>
				<!-- too-slow-encore charts -->
				<file name = "1.2.cht" type = "data" source = "iso/songs/too-slow-encore/too-slow-encore.json.cht"/>
