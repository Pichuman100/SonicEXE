                <file name = "1.9.cht" type = "data" source = "iso/songs/fight-or-flight/fight-or-flight-hard.json.cht"/>
			</dir>
			
			<!-- Music -->
			<dir name = "music">
				<file name = "menu.xa" type = "xa" source = "iso/music/menu.xa"/>
